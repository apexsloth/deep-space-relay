{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "allowImportingTsExtensions": true,
    "types": ["bun-types"]
  }
}